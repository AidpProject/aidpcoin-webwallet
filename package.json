{
  "name": "aidpcoin-webwallet",
  "version": "1.2.6",
  "description": "Web wallet for Aidpcoin",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "parcel build index.html --no-source-maps",
    "watch": "parcel watch index.html",
