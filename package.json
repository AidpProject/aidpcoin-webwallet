    "AIDP"
  ],
  "author": "Aidp Rebels / Dick Henrik Larsson",
  "license": "MIT",
  "dependencies": {
    "@aidpproject/aidpcoin-history-list": "^1.2.9",
    "@aidpproject/aidpcoin-jswallet": "^0.10.9",
