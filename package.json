    "@aidpproject/aidpcoin-message": "^0.7.5",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "cors": "^2.8.5",
    "crypto-js": "^4.2.0",
    "ember-cli-cors": "^0.0.2",
    "parcel": "^2.11.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-qr-reader": "^3.0.0-beta-1"
  },
  "overrides": {
